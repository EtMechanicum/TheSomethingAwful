import it.uniroma3.siw.siw_recipes.service.ReviewService;

@Controller
public class ReviewController {
	@Autowired
	private ReviewService rs;
	
	@PostMapping("/recipes/deleteReview/{id}")
	public String deleteReview(@PathVariable Long id, )
}
